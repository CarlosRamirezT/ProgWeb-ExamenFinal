<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Gestor Visitas</title>


    <link href="~/Content/css/materialize.css" rel="stylesheet" />
    <link href="~/Content/css/materialize.min.css" rel="stylesheet" />
    <link href="~/Content/css/myStyle-inside.css" rel="stylesheet" />

    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="icon" href="https://cdn3.iconfinder.com/data/icons/happily-colored-snlogo/128/medium.png">

</head>
<body>

    <div class="navbar-fixed">
        <nav>
            <div class="nav-wrapper">
                <a href="#!" class="brand-logo">Gestor de Visitas</a>
                <ul class="right hide-on-med-and-down">
                    <li>@Html.ActionLink("Contactos", "Index", "Contacts")</li>
                    <li>@Html.ActionLink("Visitas", "Index", "Visits")</li>
                    <li>@Html.ActionLink("Salir", "Login", "User")</li>
                </ul>
            </div>
        </nav>
    </div>

    <br />
    @RenderBody()
    <br />

    <footer>
        <p>Cadara Software - &copy; @DateTime.Now.Year Todos los Derechos Reservados</p>
    </footer>

    <script src="~/Content/js/materialize.js"></script>
    <script src="~/Content/js/materialize.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>
    <script src="mySpxript.js"></script>
</body>
</html>